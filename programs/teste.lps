Program Teste {
    int x;
    float y;
    bool z;
    str c;

    input(x);
    input(y);
    input(z);
    input(c);

    // Operadores unários
    x = -x;
    z = !z;

    // Operadores binários
    int soma = x + 10;
    float subtracao = y - 5.5;
    int multiplicacao = x * 2;
    float divisao = y / 2.0;

    // Comparações
    bool igual = (x == 10);
    bool diferente = (y != 5.5);
    bool maiorIgual = (x >= 10);
    bool menorIgual = (y <= 5.5);
    bool maior = (x > 0);
    bool menor = (y < 10.0);

    // Impressão dos resultados
    print("Soma: ", soma);
    print("Subtracao: ", subtracao);
    print("Multiplicacao: ", multiplicacao);
    print("Divisao: ", divisao);
    print("Igual: ", igual);
    print("Diferente: ", diferente);
    print("Maior ou igual: ", maiorIgual);
    print("Menor ou igual: ", menorIgual);
    print("Maior: ", maior);
    print("Menor: ", menor);
}